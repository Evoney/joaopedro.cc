  "license": "ISC",
  "main": "n/a",
  "keywords": [
    "js",
    "css",
    "blog",
    "react",
    "gatsby",
    "frontend"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "lint:fix": "eslint ./src/**/*.{js,jsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx}",
