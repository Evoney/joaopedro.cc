{
  "name": "joaopedro.cc",
  "description": "My personal blog created with Gatsby, TypeScript, ThemeUI and more",
  "private": true,
  "version": "3.0.0",
  "main": "n/a",
  "repository": "git@github.com:jpedroschmitz/joaopedro.cc.git",
  "author": "Jo√£o Pedro Schmitz <jpedroschmitz@hotmail.com> (https://joaopedro.cc)",
  "license": "ISC",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@mdx-js/react": "^1.5.5",
    "@theme-ui/components": "^0.3.1",
    "gatsby": "^2.19.23",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.16",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "theme-ui": "^0.3.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typeface-noto-serif": "^0.0.72",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/theme-ui": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-preset-gatsby": "^0.2.28",
    "commitlint": "^8.3.5",
    "commitlint-config-rocketseat": "^0.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-root-import": "^1.0.4",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": ">=4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "lint-staged": ">=10",
    "prettier": "^1.19.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  }
}
